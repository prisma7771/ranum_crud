{
    "headers": [
      {
        "source": "/_next/static/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      },
      {
        "source": "/favicon.ico",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=86400"
          }
        ]
      },
      {
        "source": "/assets/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      },
      {
        "source": "/(.*\\.webp)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      },
      {
        "source": "/pakets",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=60, stale-while-revalidate=300"
          }
        ]
      },
      {
        "source": "/galleries",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=60, stale-while-revalidate=300"
          }
        ]
      }
    ],
    "rewrites": [
      {
        "source": "/favicon.ico",
        "destination": "/favicon.ico"
      },
      {
        "source": "/assets/(.*)",
        "destination": "/assets/$1"
      },
      {
        "source": "/(.*\\.js)",
        "destination": "/$1"
      },
      {
        "source": "/(.*\\.css)",
        "destination": "/$1"
      },
      {
        "source": "/(.*\\.webp)",
        "destination": "/$1"
      },
      {
        "source": "/(.*\\.ico)",
        "destination": "/$1"
      },
      {
        "source": "/(.*)",
        "destination": "/"
      }
    ]
  }
  